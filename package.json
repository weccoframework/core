{
    "name": "@wecco/core",
    "version": "0.3.0",
    "description": "A simple, minimalistic web framework based on web components and HTML templates",
    "license": "Apache License Version 2.0",
    "devDependencies": {
        "typescript": "3.3.1",
        "mocha": "6.0.2",
        "@types/mocha": "5.2.0",
        "ts-node": "8.0.2",
        "iko": "0.6.0",
        "sinon": "7.2.6",
        "@types/sinon": "4.3.1",
        "jsdom": "13.2.0",
        "jsdom-global": "3.0.2",
        "nyc": "13.3.0",
        "rollup": "1.12.4",
        "rollup-plugin-typescript2": "0.21.1",
        "rollup-plugin-commonjs": "10.0.0",
        "puppeteer": "1.17.0",
        "@types/puppeteer": "1.12.4",
        "express": "4.17.1",
        "@types/express": "4.16.1"
    },
    "main": "dist/wecco-core.umd.js",
    "module": "dist/wecco-core.es5.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "rollup -c",
        "watch": "rollup -c -w",
        "test": "mocha --require ts-node/register --require jsdom-global/register test/**/*.spec.ts",
        "acceptance-test": "mocha --timeout 5000 test/**/acceptance.test.js",
        "coverage": "nyc mocha"
    },
    "nyc": {
        "include": [
            "src/**/*.ts"
        ],
        "extension": [
            ".ts"
        ],
        "require": [
            "ts-node/register"
        ],
        "reporter": [
            "text-summary",
            "html"
        ],
        "sourceMap": true,
        "instrument": true
    }
}